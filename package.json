  "license": "ISC",
  "dependencies": {
    "haxe-modular": "^0.1.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-router": "^3.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "cross-env": "^3.1.3",
    "envify": "^3.4.1",
    "livereloadx": "^0.3.9",
    "shx": "^0.2.0",
    "uglify-js": "^2.7.4"
  }
}
