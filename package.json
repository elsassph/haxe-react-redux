{
  "name": "haxe-react-redux",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "libs": "browserify src/libs.js -o bin/libs.js -d",
    "serve": "livereloadx -s bin",
    "dev": "haxe build.hxml -debug",
    "build": "haxe build.hxml",
    "minify": "node minify-all.js",
    "clean": "shx rm -f bin/*.js && shx rm -f bin/*.map",
